{
  "name": "itobuz/wp-twig",
  "description": "WordPress site",
  "config": {
      "vendor-dir": "vendor",
    "allow-plugins": {
      "composer/installers": true
    }
   },
  "require": {
    "timber/timber": "2.2.0",
    "cmb2/cmb2": "2.10.1",
    "wpackagist-plugin/lazy-blocks": "3.7.0",
    "wpackagist-plugin/redux-framework": "4.4.18",
    "djboris88/timber-commented-include": "2.0.0",
    "rosell-dk/webp-convert": "2.9.3"
  },
  "repositories":[
        {
          "type":"composer",
            "url":"https://wpackagist.org"
        }
   ],
  "extra": {
      "installer-paths": {
          "vendor/{$name}/": ["type:wordpress-plugin"]
      }
   },
  "autoload": {
	  "files": [
      "vendor/cmb2/init.php"
    ]
  }
}
