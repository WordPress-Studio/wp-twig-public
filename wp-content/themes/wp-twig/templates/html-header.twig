<!doctype html class=">
<!--[if lt IE 9]><html class="no-js no-svg ie lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}> <![endif]-->
<!--[if IE 9]><html class="no-js no-svg ie ie9 lt-ie9 lt-ie8" {{ site.language_attributes }}> <![endif]-->
<!--[if gt IE 9]><!--><html class="no-js no-svg" {{ site.language_attributes }}> <!--<![endif]-->
<head>
    <meta charset="{{ site.charset }}" />
    <meta name="description" content="{{ site.description }}">

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="author" href="{{ theme_link }}/humans.txt" />
    <link rel="pingback" href="{{ site.pingback_url }}" />
    <link rel="profile" href="http://gmpg.org/xfn/11">
    <link rel="icon" type="image/png" href="{{site_favicon}}" />
    {{function('wp_head')}}

    <! Global stylesheet ->
       {% for item in site_global_stylesheet_list %}
         {% if 'http' in item %}
            <link rel="stylesheet" href="{{item}}" {% if rocket_loader %}  media="none" onload="if(media!='all')media='all'"{% endif %}>
          {% else %}
            <link rel="stylesheet" href="{{ theme_link }}{{item}}" {% if rocket_loader %}  media="none" onload="if(media!='all')media='all'"{% endif %}>
          {% endif %}
      {% endfor %}

    <! // Global stylesheet ->

    <! Block Styles auto loaded ->

    {% for item in block_style_sheets %}
        {% if 'http' in item %}
			    <link rel="stylesheet" href="{{item}}" {% if rocket_loader %}  media="none" onload="if(media!='all')media='all'"{% endif %}>
		    {% else %}
			    <link rel="stylesheet" href="{{ theme_link }}{{item}}" {% if rocket_loader %}  media="none" onload="if(media!='all')media='all'"{% endif %}>
		    {% endif %}
    {% endfor %}

    <! // Block Styles auto loaded ->


    <! Styles loaed from metabox ->

     {% for item in page_specific_style_sheets %}
        {% if 'http' in item %}
			    <link rel="stylesheet" href="{{item}}" {% if rocket_loader %}  media="none" onload="if(media!='all')media='all'"{% endif %}>
		    {% else %}
			    <link rel="stylesheet" href="{{ theme_link }}{{item}}" {% if rocket_loader %}  media="none" onload="if(media!='all')media='all'"{% endif %}>
		    {% endif %}
    {% endfor %}

    <! // Styles loaed from metabox ->

<script>
   var theme_root = "{{theme_link}}";
</script>


